{
  "name": "ros1-food-delivery-web-rai-v1",
  "version": "1.0.0",
  "description": "Mini Delivery Robot Showcase of RAI Students",
  "scripts": {
    "start": "npm-run-all --parallel start-backend start-frontend",
    "start-backend": "cd backend && npm start",
    "start-frontend": "cd frontend && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
     "install-all": "npm-run-all --parallel install-backend prisma-generate install-frontend",
    "install-backend": "cd backend && npm install",
    "prisma-generate": "cd backend && npx prisma generate",
    "prisma-migrate": "cd backend && npx prisma migrate dev --name init",
    "install-frontend": "cd frontend && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/piriyapol/ROS1-food-delivery-web-rai-v1.git"
